{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": [
		"main"
	],
	"permissions": [
		"core:default",
		"dialog:allow-open",
		"dialog:allow-save",
		"dialog:allow-message",
		"fs:allow-app-read",
		"fs:allow-app-read-recursive",
		"fs:allow-app-write",
		"fs:allow-app-write-recursive"
	]
}